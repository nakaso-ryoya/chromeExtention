{
  "name": "intra-martログインツール",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": [
      "content.js"
    ]
  }],
  "browser_action": {
    "default_icon": {"19":"icon.png"},
    "default_title": "ユーザ選択",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}